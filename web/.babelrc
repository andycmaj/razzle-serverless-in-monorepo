{
  "presets": [
    [
      "razzle/babel",
      {
        "preset-env": {
          "modules": "commonjs"
        }
      }
    ]
  ],
  "plugins": [
    "after",
    [
      "styled-components",
      {
        "ssr": true,
        "displayName": true,
        "minify": false,
        "transpileTemplateLiterals": true
      }
    ]
  ]
}